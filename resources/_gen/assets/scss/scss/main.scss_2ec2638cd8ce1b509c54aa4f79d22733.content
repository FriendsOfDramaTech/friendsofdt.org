*{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}body{line-height:1.7em;color:white;background-color:black;font-size:13px}.pure-img-responsive{max-width:100%;height:auto}.l-box{padding:1em}.l-box-lrg{padding:2em;border-bottom:1px solid rgba(0,0,0,0.1)}.is-center{text-align:center}.home-menu{padding:0.5em;text-align:center;box-shadow:0 1px 1px rgba(0,0,0,0.1)}.pure-menu.pure-menu-fixed{border-bottom:none;z-index:4}.home-menu .pure-menu-heading{color:white;font-weight:400;font-size:120%}.home-menu .pure-menu-selected a{color:inherit}.home-menu a{color:inherit}.home-menu li a:hover,.home-menu li a:focus{background:none;border:none;color:inherit}.splash-container{z-index:1;overflow:hidden;width:100%;height:auto;top:0;left:0;position:fixed !important}.splash{width:80%;height:50%;margin:auto;position:absolute;top:100px;left:0;bottom:0;right:0;text-align:center;text-transform:uppercase}.splash-head{font-size:20px;font-weight:bold;border:3px solid white;padding:1em 1.6em;font-weight:100;border-radius:5px;line-height:1em}.splash-subhead{letter-spacing:0.05em;opacity:0.8}.content-wrapper{position:absolute;top:40%;width:100%;min-height:12%;z-index:2;background:black;color:#ddd}.content{padding:1em 1em 3em;background:#111}.content-head{font-weight:400;text-transform:uppercase;letter-spacing:0.1em;margin:2em 0 1em}.content-subhead i{margin-right:7px}.ribbon{background:black;color:white}.footer{background:black;position:fixed;bottom:0;width:100%}@media (min-width: 48em){body{font-size:16px}.home-menu{text-align:left}.home-menu ul{float:right}.splash{width:50%;height:50%}.splash-head{font-size:250%}.l-box-lrg{border:none}.content-wrapper{top:65%}}@media (min-width: 78em){.splash-head{font-size:300%}.content-wrapper{top:90%}}
